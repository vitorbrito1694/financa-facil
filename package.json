{
  "name": "financa-facil",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 1239",
    "build": "next build",
    "start": "next start --port 1241",
    "lint": "next lint",
    "seed": "tsx src/prisma/seed.ts",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "prisma:pull": "prisma db pull --schema=./src/prisma/schema.prisma",
    "vercel-build": "prisma generate --schema=./src/prisma/schema.prisma && prisma migrate deploy --schema=./src/prisma/schema.prisma && next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-avatar": "^1.1.10",
